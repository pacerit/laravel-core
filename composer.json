{
    "name": "pacerit/ButterflyCore",
    "description": "Package of core classes that provide standardization in usage of Services, Repositories etc. for Laravel framework.",
    "type": "library",
    "keywords": [
        "pacerit",
        "butterfly-core",
        "core",
        "framework",
        "laravel"
    ],
    "require": {
        "laravel/framework": "5.8.*",
        "php": "^7.1",
        "ext-json": "^1.5"
    },
    "license": "No license",
    "authors": [
        {
            "name": "PacerIT Development Team",
            "email": "kontakt@pacerit.pl",
            "role": "developer",
            "homepage": "https://pacerit.pl"
        }
    ],
    "autoload": {
        "psr-4": {
            "pacerit\\ButterflyCore\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "pacerit\\ButterflyCore\\Providers\\ButterflyCoreServiceProvider"
            ]
        }
    },
    "minimum-stability": "stable"
}
